			<xs:element name="height" type="numericValueType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Hoehenbeschraenkungen fuer das Verkaufsprodukt</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="diameter" type="numericValueType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Durchmesserbeschraenkungen fuer das Verkaufsprodukt</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="girth" type="numericValueType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Gurtmassbeschraenkungen fuer das Verkaufsprodukt</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="addedEdgeLength" type="numericValueType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Beschraenkungen fuer die Summe der Kantenlaengen eines Verkaufsproduktes</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="weight" type="numericValueType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Gewichtsbeschraenkungen fuer das Verkaufsprodukt</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
